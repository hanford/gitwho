{
  "version": 2,
  "env": {
    "GITHUB_ACCESS_TOKEN": "@github-access-token"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "builds": [
    {
      "src": "www/next.config.js",
      "use": "@now/next@canary",
      "config": { "maxLambdaSize": "20mb" }
    },
    {
      "src": "api/index.js",
      "use": "@now/node-server",
      "config": { "maxLambdaSize": "10mb" }
    },
    {
      "src": "www/static/*",
      "use": "@now/static"
    }
  ]
}
